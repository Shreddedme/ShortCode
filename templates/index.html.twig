{% extends 'base.html.twig' %}

 {% block body %}
     <!DOCTYPE html>
     <html lang="en">
     <head>
         <meta charset="UTF-8">
         <title></title>
         <link rel="stylesheet" href="styles.css"/>
     </head>
     <body>
     <header>
         {% if is_granted('IS_AUTHENTICATED_FULLY') %}
             <button class="green"><a href="{{ path('app_logout') }}">Выйти</a></button>
         {% else %}
             <button class="green"><a href="{{ path('app_login') }}">Войти</a></button>
             <button class="green"><a href="{{ path('app_register') }}">Регистрация</a></button>
         {% endif %}

     <main>
     {% block main_cont %}

         {% endblock %}
     </main>
 {% endblock %}